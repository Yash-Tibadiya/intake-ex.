{
  "title": "Comprehensive Health Intake Form",
  "description": "Complete health and wellness assessment",
  "version": "1.0",
  "settings": {
    "allowBack": true,
    "showProgress": true,
    "autoSave": true,
    "theme": "healthcare"
  },
  "pages": [
    {
      "id": "welcome",
      "header": "Welcome to Your Health Assessment",
      "subheader": "This comprehensive form will help us understand your health needs and provide personalized care.",
      "ctaText": "Get Started",
      "previousButtonText": "",
      "questions": [
        {
          "id": "consent",
          "type": "checkbox",
          "label": "Privacy and Consent",
          "hint": "Please review and accept our terms to continue",
          "required": true,
          "options": [
            {
              "value": "privacy_policy",
              "label": "I have read and agree to the Privacy Policy"
            },
            {
              "value": "terms_service",
              "label": "I agree to the Terms of Service"
            },
            {
              "value": "marketing_emails",
              "label": "I consent to receiving marketing emails (optional)"
            }
          ]
        }
      ]
    },
    {
      "id": "personal_info",
      "header": "Personal Information",
      "subheader": "Let's start with some basic information about you",
      "questions": [
        {
          "id": "full_name",
          "type": "text",
          "label": "Full Name",
          "placeholder": "Enter your full name",
          "required": true,
          "maxLength": 100,
          "validation": {
            "rules": ["required", "minLength:2"]
          }
        },
        {
          "id": "date_of_birth",
          "type": "date",
          "label": "Date of Birth",
          "required": true,
          "validation": {
            "rules": ["required"]
          }
        },
        {
          "id": "sex_assigned_birth",
          "type": "radio",
          "label": "What was your sex assigned at birth?",
          "hint": "This information helps us provide appropriate medical guidance",
          "required": true,
          "options": [
            { "value": "male", "label": "Male" },
            { "value": "female", "label": "Female" },
            { "value": "intersex", "label": "Intersex" },
            { "value": "decline", "label": "Decline to answer" }
          ]
        },
        {
          "id": "gender_identity",
          "type": "select",
          "label": "Gender Identity",
          "placeholder": "Select your gender identity",
          "required": false,
          "options": [
            { "value": "man", "label": "Man" },
            { "value": "woman", "label": "Woman" },
            { "value": "non_binary", "label": "Non-binary" },
            { "value": "transgender_man", "label": "Transgender man" },
            { "value": "transgender_woman", "label": "Transgender woman" },
            { "value": "genderfluid", "label": "Genderfluid" },
            { "value": "other", "label": "Other" },
            { "value": "decline", "label": "Prefer not to answer" }
          ],
          "subQuestions": [
            {
              "id": "gender_specify",
              "type": "text",
              "label": "Please specify your gender identity",
              "placeholder": "Enter your gender identity",
              "required": true,
              "showIf": {
                "triggerValue": "other"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "physical_measurements",
      "header": "Physical Measurements",
      "subheader": "These measurements help us calculate important health metrics",
      "questions": [
        {
          "id": "height_feet",
          "type": "number",
          "label": "Height (Feet)",
          "placeholder": "5",
          "required": true,
          "min": 3,
          "max": 8
        },
        {
          "id": "height_inches",
          "type": "number",
          "label": "Height (Inches)",
          "placeholder": "10",
          "required": true,
          "min": 0,
          "max": 11
        },
        {
          "id": "weight",
          "type": "number",
          "label": "Current Weight (lbs)",
          "placeholder": "Enter your weight",
          "required": true,
          "min": 50,
          "max": 800
        },
        {
          "id": "weight_goal",
          "type": "radio",
          "label": "What is your weight goal?",
          "required": true,
          "options": [
            { "value": "lose", "label": "Lose weight" },
            { "value": "gain", "label": "Gain weight" },
            { "value": "maintain", "label": "Maintain current weight" },
            { "value": "build_muscle", "label": "Build muscle" }
          ],
          "subQuestions": [
            {
              "id": "target_weight_loss",
              "type": "number",
              "label": "How much weight would you like to lose? (lbs)",
              "placeholder": "Enter target weight loss",
              "min": 1,
              "max": 200,
              "required": true,
              "showIf": {
                "triggerValue": "lose"
              }
            },
            {
              "id": "target_weight_gain",
              "type": "number",
              "label": "How much weight would you like to gain? (lbs)",
              "placeholder": "Enter target weight gain",
              "min": 1,
              "max": 100,
              "required": true,
              "showIf": {
                "triggerValue": "gain"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "medical_history",
      "header": "Medical History",
      "subheader": "Help us understand your medical background and current health status",
      "questions": [
        {
          "id": "current_conditions",
          "type": "checkbox",
          "label": "Do you currently have any of the following conditions?",
          "hint": "Select all that apply to you",
          "required": false,
          "options": [
            { "value": "diabetes", "label": "Diabetes" },
            { "value": "hypertension", "label": "High blood pressure" },
            { "value": "heart_disease", "label": "Heart disease" },
            { "value": "asthma", "label": "Asthma" },
            { "value": "arthritis", "label": "Arthritis" },
            { "value": "depression", "label": "Depression" },
            { "value": "anxiety", "label": "Anxiety" },
            { "value": "thyroid", "label": "Thyroid disorder" },
            { "value": "other", "label": "Other condition" },
            { "value": "none", "label": "None of the above" }
          ],
          "subQuestions": [
            {
              "id": "other_condition_details",
              "type": "textarea",
              "label": "Please describe your other medical condition(s)",
              "placeholder": "Provide details about your condition, when diagnosed, current treatment, etc.",
              "required": true,
              "rows": 4,
              "showIf": {
                "triggerValue": "other"
              }
            }
          ]
        },
        {
          "id": "current_medications",
          "type": "textarea",
          "label": "Current Medications",
          "hint": "List all medications, supplements, and vitamins you're currently taking",
          "placeholder": "Include prescription medications, over-the-counter drugs, supplements, vitamins, etc.",
          "required": false,
          "rows": 4
        },
        {
          "id": "allergies",
          "type": "text",
          "label": "Known Allergies",
          "placeholder": "List any known allergies (medications, foods, environmental, etc.)",
          "required": false,
          "maxLength": 500
        },
        {
          "id": "family_history",
          "type": "checkbox",
          "label": "Family Medical History",
          "hint": "Select conditions that run in your immediate family (parents, siblings, children)",
          "required": false,
          "options": [
            { "value": "heart_disease", "label": "Heart disease" },
            { "value": "diabetes", "label": "Diabetes" },
            { "value": "cancer", "label": "Cancer" },
            { "value": "stroke", "label": "Stroke" },
            { "value": "mental_health", "label": "Mental health conditions" },
            { "value": "autoimmune", "label": "Autoimmune disorders" },
            { "value": "none_known", "label": "None known" }
          ]
        }
      ]
    },
    {
      "id": "lifestyle",
      "header": "Lifestyle Assessment",
      "subheader": "Tell us about your daily habits and lifestyle choices",
      "questions": [
        {
          "id": "exercise_frequency",
          "type": "select",
          "label": "How often do you exercise?",
          "placeholder": "Select exercise frequency",
          "required": true,
          "options": [
            { "value": "never", "label": "Never" },
            { "value": "rarely", "label": "Rarely (less than once a week)" },
            { "value": "sometimes", "label": "Sometimes (1-2 times per week)" },
            { "value": "regularly", "label": "Regularly (3-4 times per week)" },
            { "value": "daily", "label": "Daily or almost daily" }
          ]
        },
        {
          "id": "exercise_types",
          "type": "checkbox",
          "label": "What types of exercise do you do?",
          "hint": "Select all that apply",
          "required": false,
          "showIf": {
            "questionId": "exercise_frequency",
            "operator": "not_equals",
            "value": "never"
          },
          "options": [
            {
              "value": "cardio",
              "label": "Cardio (running, cycling, swimming)"
            },
            { "value": "strength", "label": "Strength training" },
            { "value": "yoga", "label": "Yoga/Pilates" },
            { "value": "sports", "label": "Sports" },
            { "value": "walking", "label": "Walking/Hiking" },
            { "value": "dance", "label": "Dancing" },
            { "value": "other", "label": "Other" }
          ],
          "subQuestions": [
            {
              "id": "other_exercise",
              "type": "text",
              "label": "Please specify other exercise types",
              "placeholder": "Describe your other exercise activities",
              "required": true,
              "showIf": {
                "triggerValue": "other"
              }
            }
          ]
        },
        {
          "id": "smoking_status",
          "type": "radio",
          "label": "What is your smoking status?",
          "required": true,
          "options": [
            { "value": "never", "label": "Never smoked" },
            { "value": "former", "label": "Former smoker" },
            { "value": "current", "label": "Current smoker" },
            { "value": "social", "label": "Social smoker" }
          ],
          "subQuestions": [
            {
              "id": "quit_date",
              "type": "date",
              "label": "When did you quit smoking?",
              "required": true,
              "showIf": {
                "triggerValue": "former"
              }
            },
            {
              "id": "cigarettes_per_day",
              "type": "number",
              "label": "How many cigarettes do you smoke per day on average?",
              "placeholder": "Number of cigarettes",
              "min": 1,
              "max": 100,
              "required": true,
              "showIf": {
                "triggerValue": "current"
              }
            },
            {
              "id": "social_smoking_frequency",
              "type": "select",
              "label": "How often do you smoke socially?",
              "required": true,
              "options": [
                { "value": "weekly", "label": "Weekly" },
                { "value": "monthly", "label": "Monthly" },
                { "value": "occasionally", "label": "Occasionally" },
                { "value": "rarely", "label": "Rarely" }
              ],
              "showIf": {
                "triggerValue": "social"
              }
            }
          ]
        },
        {
          "id": "alcohol_consumption",
          "type": "select",
          "label": "How often do you drink alcohol?",
          "placeholder": "Select frequency",
          "required": true,
          "options": [
            { "value": "never", "label": "Never" },
            { "value": "rarely", "label": "Rarely" },
            { "value": "weekly", "label": "1-2 times per week" },
            { "value": "several_weekly", "label": "3-6 times per week" },
            { "value": "daily", "label": "Daily" }
          ]
        },
        {
          "id": "sleep_hours",
          "type": "number",
          "label": "How many hours of sleep do you get per night on average?",
          "placeholder": "Hours of sleep",
          "required": true,
          "min": 1,
          "max": 16
        },
        {
          "id": "stress_level",
          "type": "radio",
          "label": "How would you rate your current stress level?",
          "required": true,
          "options": [
            { "value": "1", "label": "Very low stress" },
            { "value": "2", "label": "Low stress" },
            { "value": "3", "label": "Moderate stress" },
            { "value": "4", "label": "High stress" },
            { "value": "5", "label": "Very high stress" }
          ]
        }
      ]
    },
    {
      "id": "healthcare_provider",
      "header": "Healthcare Provider Selection",
      "subheader": "Help us match you with the right healthcare provider",
      "questions": [
        {
          "id": "provider_type",
          "type": "radio",
          "label": "What type of healthcare provider are you looking for?",
          "required": true,
          "options": [
            { "value": "primary_care", "label": "Primary Care Physician" },
            { "value": "specialist", "label": "Specialist" },
            { "value": "mental_health", "label": "Mental Health Provider" },
            { "value": "not_sure", "label": "Not sure" }
          ],
          "subQuestions": [
            {
              "id": "specialist_type",
              "type": "searchable_dropdown",
              "label": "What type of specialist?",
              "placeholder": "Search for specialist type...",
              "required": true,
              "showIf": {
                "triggerValue": "specialist"
              },
              "options": [
                { "value": "cardiology", "label": "Cardiology (Heart)" },
                { "value": "dermatology", "label": "Dermatology (Skin)" },
                {
                  "value": "endocrinology",
                  "label": "Endocrinology (Hormones/Diabetes)"
                },
                {
                  "value": "gastroenterology",
                  "label": "Gastroenterology (Digestive)"
                },
                {
                  "value": "neurology",
                  "label": "Neurology (Brain/Nervous System)"
                },
                {
                  "value": "orthopedics",
                  "label": "Orthopedics (Bones/Joints)"
                },
                { "value": "psychiatry", "label": "Psychiatry" },
                { "value": "pulmonology", "label": "Pulmonology (Lungs)" },
                {
                  "value": "rheumatology",
                  "label": "Rheumatology (Autoimmune)"
                },
                { "value": "urology", "label": "Urology" },
                { "value": "other_specialist", "label": "Other specialist" }
              ]
            },
            {
              "id": "mental_health_type",
              "type": "checkbox",
              "label": "What type of mental health support are you seeking?",
              "hint": "Select all that apply",
              "required": true,
              "showIf": {
                "triggerValue": "mental_health"
              },
              "options": [
                {
                  "value": "therapy",
                  "label": "Individual therapy/counseling"
                },
                { "value": "couples_therapy", "label": "Couples therapy" },
                { "value": "family_therapy", "label": "Family therapy" },
                { "value": "group_therapy", "label": "Group therapy" },
                {
                  "value": "medication_management",
                  "label": "Medication management"
                },
                { "value": "crisis_support", "label": "Crisis support" }
              ]
            }
          ]
        },
        {
          "id": "preferred_doctor",
          "type": "searchable_dropdown",
          "label": "Do you have a preferred doctor?",
          "placeholder": "Search for your preferred doctor...",
          "required": false,
          "options": [
            {
              "value": "dr_smith",
              "label": "Dr. John Smith - Internal Medicine"
            },
            {
              "value": "dr_johnson",
              "label": "Dr. Sarah Johnson - Family Medicine"
            },
            {
              "value": "dr_williams",
              "label": "Dr. Michael Williams - Cardiology"
            },
            {
              "value": "dr_brown",
              "label": "Dr. Emily Brown - Family Medicine"
            },
            {
              "value": "dr_davis",
              "label": "Dr. Robert Davis - Internal Medicine"
            },
            {
              "value": "dr_wilson",
              "label": "Dr. Lisa Wilson - Endocrinology"
            },
            {
              "value": "dr_martinez",
              "label": "Dr. Carlos Martinez - Psychiatry"
            },
            {
              "value": "dr_taylor",
              "label": "Dr. Amanda Taylor - Dermatology"
            },
            { "value": "no_preference", "label": "No preference" }
          ]
        },
        {
          "id": "insurance_provider",
          "type": "searchable_dropdown",
          "label": "Insurance Provider",
          "placeholder": "Search for your insurance provider...",
          "required": true,
          "options": [
            { "value": "aetna", "label": "Aetna" },
            { "value": "anthem", "label": "Anthem Blue Cross Blue Shield" },
            { "value": "cigna", "label": "Cigna" },
            { "value": "humana", "label": "Humana" },
            { "value": "kaiser", "label": "Kaiser Permanente" },
            { "value": "bcbs", "label": "Blue Cross Blue Shield" },
            { "value": "united", "label": "UnitedHealthcare" },
            { "value": "medicare", "label": "Medicare" },
            { "value": "medicaid", "label": "Medicaid" },
            { "value": "self_pay", "label": "Self-pay/No insurance" },
            { "value": "other", "label": "Other insurance" }
          ],
          "subQuestions": [
            {
              "id": "other_insurance",
              "type": "text",
              "label": "Please specify your insurance provider",
              "placeholder": "Enter your insurance provider name",
              "required": true,
              "showIf": {
                "triggerValue": "other"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "contact_info",
      "header": "Contact Information",
      "subheader": "How would you like us to reach you?",
      "questions": [
        {
          "id": "email",
          "type": "email",
          "label": "Email Address",
          "placeholder": "your.email@example.com",
          "required": true,
          "hint": "We'll use this for appointment confirmations and health updates",
          "validation": {
            "rules": ["required", "email"]
          }
        },
        {
          "id": "phone_primary",
          "type": "text",
          "label": "Primary Phone Number",
          "placeholder": "(555) 123-4567",
          "required": true,
          "pattern": "\\(\\d{3}\\)\\s\\d{3}-\\d{4}",
          "validation": {
            "rules": ["required", "phone"]
          }
        },
        {
          "id": "phone_secondary",
          "type": "text",
          "label": "Secondary Phone Number (Optional)",
          "placeholder": "(555) 123-4567",
          "required": false
        },
        {
          "id": "contact_preferences",
          "type": "checkbox",
          "label": "Preferred contact methods",
          "hint": "Select all methods you're comfortable with",
          "required": true,
          "options": [
            { "value": "email", "label": "Email" },
            { "value": "phone_call", "label": "Phone call" },
            { "value": "text_message", "label": "Text message" },
            { "value": "patient_portal", "label": "Patient portal" },
            { "value": "mail", "label": "Physical mail" }
          ]
        },
        {
          "id": "best_contact_time",
          "type": "select",
          "label": "Best time to contact you",
          "placeholder": "Select preferred time",
          "required": false,
          "options": [
            { "value": "early_morning", "label": "Early Morning (6AM - 9AM)" },
            { "value": "morning", "label": "Morning (9AM - 12PM)" },
            { "value": "afternoon", "label": "Afternoon (12PM - 5PM)" },
            { "value": "evening", "label": "Evening (5PM - 8PM)" },
            { "value": "late_evening", "label": "Late Evening (8PM - 10PM)" },
            { "value": "anytime", "label": "Anytime during business hours" }
          ]
        },
        {
          "id": "emergency_contact",
          "type": "text",
          "label": "Emergency Contact",
          "placeholder": "Name and phone number of emergency contact",
          "required": true,
          "hint": "Please provide name and phone number"
        }
      ]
    },
    {
      "id": "documents",
      "header": "Document Upload",
      "subheader": "Please upload any relevant documents to help us serve you better",
      "questions": [
        {
          "id": "insurance_card",
          "type": "file_upload",
          "label": "Insurance Card",
          "hint": "Please upload clear photos of the front and back of your insurance card",
          "required": true,
          "multiple": true,
          "allowedFormats": ["jpg", "jpeg", "png", "pdf"],
          "uploadText": "Upload insurance card images",
          "maxSize": 10
        },
        {
          "id": "identification",
          "type": "file_upload",
          "label": "Photo ID",
          "hint": "Upload a clear photo of your driver's license or government-issued ID",
          "required": true,
          "multiple": false,
          "allowedFormats": ["jpg", "jpeg", "png", "pdf"],
          "uploadText": "Upload photo ID",
          "maxSize": 5
        },
        {
          "id": "medical_records",
          "type": "file_upload",
          "label": "Recent Medical Records (Optional)",
          "hint": "Upload any recent test results, medical records, or relevant documents that might help us provide better care",
          "required": false,
          "multiple": true,
          "allowedFormats": ["pdf", "jpg", "jpeg", "png", "doc", "docx"],
          "uploadText": "Drop medical documents here or click to browse",
          "maxSize": 25
        },
        {
          "id": "prescription_list",
          "type": "file_upload",
          "label": "Current Prescription List (Optional)",
          "hint": "If you have a current medication list from your pharmacy or doctor, please upload it",
          "required": false,
          "multiple": false,
          "allowedFormats": ["pdf", "jpg", "jpeg", "png", "doc", "docx"],
          "uploadText": "Upload prescription list",
          "maxSize": 5
        }
      ]
    },
    {
      "id": "health_goals",
      "header": "Health Goals & Preferences",
      "subheader": "Help us understand what you want to achieve with your healthcare",
      "questions": [
        {
          "id": "primary_health_concern",
          "type": "textarea",
          "label": "What is your primary health concern or reason for seeking care?",
          "placeholder": "Describe your main health concern, symptoms, or what brought you here today...",
          "required": true,
          "rows": 4,
          "maxLength": 1000
        },
        {
          "id": "health_goals",
          "type": "checkbox",
          "label": "What are your health and wellness goals?",
          "hint": "Select all that apply to you",
          "required": false,
          "options": [
            { "value": "weight_management", "label": "Weight management" },
            {
              "value": "fitness_improvement",
              "label": "Improve fitness level"
            },
            { "value": "energy_boost", "label": "Increase energy levels" },
            { "value": "sleep_quality", "label": "Improve sleep quality" },
            {
              "value": "stress_management",
              "label": "Better stress management"
            },
            {
              "value": "chronic_condition",
              "label": "Manage chronic condition"
            },
            {
              "value": "preventive_care",
              "label": "Preventive care and screening"
            },
            { "value": "pain_management", "label": "Pain management" },
            { "value": "mental_health", "label": "Mental health support" },
            { "value": "nutrition", "label": "Improve nutrition" },
            { "value": "other", "label": "Other goals" }
          ],
          "subQuestions": [
            {
              "id": "other_health_goals",
              "type": "textarea",
              "label": "Please describe your other health goals",
              "placeholder": "Tell us about your specific health and wellness goals...",
              "rows": 3,
              "required": true,
              "maxLength": 500,
              "showIf": {
                "triggerValue": "other"
              }
            }
          ]
        },
        {
          "id": "treatment_preferences",
          "type": "checkbox",
          "label": "Treatment Preferences",
          "hint": "How do you prefer to approach your healthcare?",
          "required": false,
          "options": [
            { "value": "conventional", "label": "Conventional medicine" },
            { "value": "holistic", "label": "Holistic/integrative approach" },
            { "value": "natural", "label": "Natural remedies when possible" },
            { "value": "preventive", "label": "Focus on prevention" },
            {
              "value": "evidence_based",
              "label": "Evidence-based treatments only"
            },
            {
              "value": "collaborative",
              "label": "Collaborative decision making"
            }
          ]
        },
        {
          "id": "appointment_urgency",
          "type": "radio",
          "label": "How urgent is your need for an appointment?",
          "required": true,
          "options": [
            {
              "value": "emergency",
              "label": "Emergency (need immediate care)"
            },
            { "value": "urgent", "label": "Urgent (within 1-2 days)" },
            { "value": "soon", "label": "Soon (within 1 week)" },
            { "value": "routine", "label": "Routine (within 2-4 weeks)" },
            { "value": "flexible", "label": "Flexible timing" }
          ]
        }
      ]
    },
    {
      "id": "custom_components",
      "header": "Health Assessment Tools",
